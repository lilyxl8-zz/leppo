<h3>Categories</h3>
<ul>
  <% categories.each(function (category) { %>
    <li>
    <a href="#/categories/<%= category.id %>"><%= category.escape("title") %></a>
    <a href="#/categories/<%= category.id %>/edit">Edit</a>
    <% console.log(category) %>
    </li>
  <% }); %>

</ul>
<a href="#/categories/new">New Category</a>
