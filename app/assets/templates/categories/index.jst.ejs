<h1>Categories</h1>
<ul class="categories-list">
  <% categories.each(function (category) { %>
    <li class="category-item">
    <a href="#/categories/<%= category.escape("title").toLowerCase() %>"><%= category.escape("title") %></a>
    </li>
  <% }); %>

</ul>
<p>
  <a href="#/categories/new">Add Category</a><br>
  <a href="#/feeds/new">Add Feed</a>
</p>
